{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "yingge",
  "version": "0.1.0",
  "identifier": "com.yingge.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:14201",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "莺歌",
        "width": 1000,
        "height": 680,
        "decorations": false,
        "transparent": false,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
